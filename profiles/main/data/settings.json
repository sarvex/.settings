{
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "dev.containers.gpuAvailability": "all",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line-thin",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.find.autoFindInSelection": "multiline",
  "editor.foldingImportsByDefault": true,
  "editor.foldingMaximumRegions": 9999,
  "editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Cascadia Code', 'CascadiaCode', 'Cascadia Code PL', 'CascadiaCodePL', 'MesloLGMDZ Nerd Font', Menlo, Monaco, Consolas,  'Courier New', monospace",
  "editor.fontLigatures": "'zero', 'onum', 'liga', 'clig', 'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'ss10', 'ss11', 'ss12', 'ss13', 'ss14', 'ss15', 'ss16', 'ss17', 'ss18', 'ss19', 'ss20'",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.indentSize": "tabSize",
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.autohide": true,
  "editor.parameterHints.cycle": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.preview": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "string",
          "parameter",
          "entity.name.type.class",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.class.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": "bold italic"
        }
      }
    ]
  },
  "editor.unusualLineTerminators": "auto",
  "editor.wordWrapColumn": 120,
  "editor.wrappingStrategy": "advanced",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "eslint.lintTask.enable": true,
  "eslint.probe": [
    "astro",
    "html",
    "htmx",
    "javascript",
    "javascriptreact",
    "markdown",
    "markdownreact",
    "svelte",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "eslint.problems.shortenToSingleLine": true,
  "eslint.run": "onSave",
  "eslint.useESLintClass": true,
  "eslint.useFlatConfig": true,
  "eslint.validate": [
    "astro",
    "html",
    "htmx",
    "javascript",
    "javascriptreact",
    "markdown",
    "markdownreact",
    "svelte",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoGuessEncoding": true,
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.autoStash": true,
  "git.branchRandomName.dictionary": ["adjectives", "animals", "colors"],
  "git.branchRandomName.enable": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.mergeEditor": true,
  "git.postCommitCommand": "sync",
  "git.pruneOnFetch": true,
  "git.rebaseWhenSync": true,
  "git.rememberPostCommitCommand": true,
  "git.terminalGitEditor": true,
  "git.useCommitInputAsStashMessage": true,
  "github.copilot.selectedCompletionModel": "gpt-4o-copilot",
  "githubLocalActions.dockerDesktopPath": "/Applications/Docker.app",
  "gitlens.advanced.messages": {
    "suppressIntegrationRequestTimedOutWarning": true
  },
  "gitlens.telemetry.enabled": false,
  "mdb.sendTelemetry": false,
  "npm.packageManager": "pnpm",
  "peacock.affectActivityBar": false,
  "peacock.affectStatusAndTitleBorders": true,
  "peacock.affectTabActiveBorder": true,
  "peacock.affectTitleBar": false,
  "peacock.showColorInStatusBar": false,
  "peacock.surpriseMeOnStartup": true,
  "prettier.bracketSameLine": true,
  "prettier.documentSelectors": ["**/*.astro", "**/*.vue", "**/*.svelte"],
  "prettier.jsxSingleQuote": true,
  "prettier.prettierPath": "~/Library/pnpm/global/5/node_modules/prettier",
  "prettier.printWidth": 120,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "python.createEnvironment.trigger": "off",
  "search.quickOpen.includeSymbols": true,
  "search.seedWithNearestWord": true,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.useGlobalIgnoreFiles": true,
  "search.useParentIgnoreFiles": true,
  "solidity.telemetry": false,
  "sonarlint.connectedMode.connections.sonarqube": [
    {
      "connectionId": "http-localhost-9000",
      "serverUrl": "http://localhost:9000"
    }
  ],
  "sonarlint.disableTelemetry": true,
  "stylelint.packageManager": "pnpm",
  "stylelint.snippet": [
    "astro",
    "css",
    "html",
    "javascript",
    "javascriptreact",
    "less",
    "markdown",
    "mdx",
    "postcss",
    "sass",
    "scss",
    "source.css.styled",
    "source.markdown.math",
    "styled-css",
    "sugarss",
    "svelte",
    "typescript",
    "typescriptreact",
    "vue-html",
    "vue-postcss",
    "vue",
    "xml",
    "xsl"
  ],
  "stylelint.validate": [
    "astro",
    "css",
    "html",
    "javascript",
    "javascriptreact",
    "less",
    "markdown",
    "mdx",
    "postcss",
    "sass",
    "scss",
    "source.css.styled",
    "source.markdown.math",
    "styled-css",
    "sugarss",
    "svelte",
    "typescript",
    "typescriptreact",
    "vue-html",
    "vue-postcss",
    "vue",
    "xml",
    "xsl"
  ],
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "LiberationMono Nerd Font Mono, LiberationMonoNerdFontMono, LiterationMono Nerd Font Mono, LiterationMonoNerdFontMono",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.scrollback": 99999,
  "terminal.integrated.shellIntegration.history": 999,
  "terminal.integrated.tabs.defaultColor": "terminal.ansiCyan",
  "terminal.integrated.tabs.showActions": "always",
  "terminal.integrated.tabs.showActiveTerminal": "always",
  "terminal.integrated.tabs.title": "${sequence}",
  "terminal.integrated.tabStopWidth": 2,
  "window.commandCenter": true,
  "window.density.editorTabHeight": "compact",
  "window.restoreFullscreen": true,
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.editor.defaultBinaryEditor": "hexEditor.hexedit",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.enableExperiments": false,
  "workbench.list.smoothScrolling": true,
  "vscode-edge-devtools.webhintInstallNotification": true,
  "workbench.colorTheme": "Catppuccin Mocha",
  "tabnine.experimentalAutoImports": true,
  "snyk.advanced.cliReleaseChannel": "preview",
  "snyk.folderConfigs": [
    {
      "folderPath": "/Users/sarvex/Sources/aysnc-rust",
      "baseBranch": ""
    }
  ],
  "workbench.activityBar.location": "top",
  "snyk.trustedFolders": ["/Users/sarvex/Sources/fahrenheit-celcius", "/Users/sarvex/Sources/aysnc-rust"],
  "cSpell.userWords": ["actix", "healthz"],
  "codeium.enableCodeLens": false,
  "rust-analyzer.lens.enable": false,
  "yaml.schemas": {
    "/Users/sarvex/.vscodium-insiders/extensions/continue.continue-1.1.24-darwin-arm64/config-yaml-schema.json": [
      ".continue/**/*.yaml"
    ]
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.iconTheme": "catppuccin-mocha",
  "terminal.integrated.env.osx": {}
}
